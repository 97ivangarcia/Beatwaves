<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".datossubida">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="814dp"
        android:layout_height="520dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/skrillex" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="413dp"
        android:layout_height="133dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.566"
        app:srcCompat="@drawable/esquinasredondas" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="178dp"
        android:layout_height="75dp"
        android:text="Skrillex"
        android:textColor="#FFFFFF"
        android:textSize="50dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView28"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/generoartista"
        android:layout_width="121dp"
        android:layout_height="46dp"
        android:backgroundTint="#FD00E4"
        android:text="Dubstep"
        android:textAlignment="center"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />


    <TextView
        android:id="@+id/textotitulo"
        android:layout_width="195dp"
        android:layout_height="33dp"
        android:text="Título de la canción:"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView28"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/textotitulo2"
        android:layout_width="195dp"
        android:layout_height="33dp"
        android:text="Album:"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView28"
        app:layout_constraintVertical_bias="0.291" />

    <TextView
        android:id="@+id/textoimagenportada"
        android:layout_width="195dp"
        android:layout_height="33dp"
        android:text="Imagen de portada:"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView28"
        app:layout_constraintVertical_bias="0.544" />

    <TextView
        android:id="@+id/textosubir"
        android:layout_width="195dp"
        android:layout_height="33dp"
        android:text="Subir canción:"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView28"
        app:layout_constraintVertical_bias="0.742" />

    <TextView
        android:id="@+id/textosubir2"
        android:layout_width="68dp"
        android:layout_height="31dp"
        android:text="Precio:"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView28"
        app:layout_constraintVertical_bias="0.937" />

    <EditText
        android:id="@+id/textoprecio"
        android:layout_width="384dp"
        android:layout_height="27dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:backgroundTintMode="screen"
        android:ems="10"
        android:hint="Título"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textotitulo"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edittextalbum"
        android:layout_width="384dp"
        android:layout_height="27dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:backgroundTintMode="screen"
        android:ems="10"
        android:hint="Álbum"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textotitulo"
        app:layout_constraintVertical_bias="0.298" />

    <EditText
        android:id="@+id/editprecio"
        android:layout_width="50dp"
        android:layout_height="31dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:backgroundTintMode="screen"
        android:ems="10"
        android:hint="€"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.232"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textotitulo"
        app:layout_constraintVertical_bias="0.919" />

    <Button
        android:id="@+id/iniciarsiguiente"
        android:layout_width="80dp"
        android:layout_height="34dp"
        android:layout_marginEnd="104dp"
        android:backgroundTint="#FD00E4"
        android:text="+"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.158"
        app:layout_constraintStart_toEndOf="@+id/textotitulo3"
        app:layout_constraintTop_toBottomOf="@+id/edittextalbum"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/iniciarsiguiente2"
        android:layout_width="80dp"
        android:layout_height="34dp"
        android:layout_marginEnd="104dp"
        android:backgroundTint="#FD00E4"
        android:text="+"
        app:layout_constraintBottom_toTopOf="@+id/barranavegacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.158"
        app:layout_constraintStart_toEndOf="@+id/textotitulo3"
        app:layout_constraintTop_toBottomOf="@+id/edittextalbum"
        app:layout_constraintVertical_bias="0.439" />

    <LinearLayout
        android:id="@+id/barranavegacion"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#333"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        >

        <!-- Botón Home -->
        <ImageButton
            android:id="@+id/botoncatalogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:src="@drawable/homepng" />

        <!-- Botón Heart -->
        <ImageButton
            android:id="@+id/navigation_heart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:src="@drawable/heartpng" />

        <!-- Botón Menu -->
        <ImageButton
            android:id="@+id/botonsubida"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:src="@drawable/menupng" />

        <!-- Botón Shopping Bag -->
        <ImageButton
            android:id="@+id/botonpago"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:src="@drawable/shoppingbagpng" />

        <!-- Botón User -->
        <ImageButton
            android:id="@+id/botonperfil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:src="@drawable/userpng" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>